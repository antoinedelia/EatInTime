<Window x:Class="EatInTimeAdmin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EatInTimeAdmin"
        xmlns:p="clr-namespace:EatInTimeAdmin.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <p:ViewModelAdmin/>
    </Window.DataContext>
    <Grid>
        <TabControl x:Name="AdminTabControl">
            <TabItem x:Name="ManageDishes">
                <DataGrid x:Name="DishesDataGrid" ItemsSource="{Binding Commande}" >
                </DataGrid>
            </TabItem>
            <TabItem x:Name="ManageTables">
                
            </TabItem>
        </TabControl>
    </Grid>
</Window>
